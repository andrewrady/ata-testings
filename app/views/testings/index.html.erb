<p id="notice"><%= notice %></p>

<h1>Testings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3">Date</th>
      <th>Status</th>
      <th>Show</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>

  <tbody>
    <% @testings.each do |testing| %>
      <tr>
        <td colspan="3"><%= testing.date.strftime("%m/%d/%Y") %></td>
        <td><%= testing.status ? "Yes" : "No" %></td>
        <td><%= link_to 'Show', testing %></td>
        <td><%= link_to 'Edit', edit_testing_path(testing) %></td>
        <td><%= link_to 'Destroy', testing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Testing', new_testing_path %>
